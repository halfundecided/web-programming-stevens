<section>
    <h1>People Found</h1>
    <h2>{{personName}}</h2>
    <main>
        <dl>
            <dt>ID</dt>
            <dd>{{_id}}</dd>
            <dt>First Name</dt>
            <dd>{{firstName}}</dd>
            <dt>Last Name</dt>
            <dd>{{lastName}}</dd>
            <dt>Address</dt>
            <dd>{{address}}</dd>
            <dt>Zip Code</dt>
            <dd>{{zip}}</dd>
            <dt>Phone</dt>
            <dd>{{phone}}</dd>
            <dt>Social Security Number</dt>
            <dd>{{ssn}}</dd>
        </dl>
    </main>
</section>